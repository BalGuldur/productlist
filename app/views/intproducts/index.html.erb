<h1>Listing intproducts</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Partnumber</th>
      <th>Serialnumber</th>
      <th>Prodtype</th>
      <th>Employee</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @intproducts.each do |intproduct| %>
      <tr>
        <td><%= intproduct.name %></td>
        <td><%= intproduct.partnumber %></td>
        <td><%= intproduct.serialnumber %></td>
        <td><%= intproduct.prodtype.name %></td>
        <td><%= intproduct.employee.fio %></td>
        <td><%= link_to 'Show', intproduct %></td>
        <td><%= link_to 'Edit', edit_intproduct_path(intproduct) %></td>
        <td><%= link_to 'Destroy', intproduct, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Intproduct', new_intproduct_path %>
